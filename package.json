{
    "name": "usemobx",
    "description": "mobx hooks for react",
    "version": "0.1.0",
    "author": "Denis Churbanov",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/iminside/usemobx.git"
    },
    "type": "module",
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "start": "rollup -c -w",
